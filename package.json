{
  "name": "yarb",
  "version": "1.0.0",
  "private": true,
  "description": "yet another react boilerplate...",
  "license": "MIT",
  "scripts": {
    "dev": "node util/cli.js dev",
    "stage": "node util/cli.js stage",
    "start": "node util/cli.js serve",
    "build": "node util/cli.js build",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "format": "prettier --ignore-path ./.gitignore --write \"**/*.js\""
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.0-beta.2",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.11",
    "detect-port": "^1.2.2",
    "eslint": "^4.19.1",
    "eslint-config-dgeibi": "^4.1.0",
    "finalhandler": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "internal-ip": "^3.0.1",
    "koa-connect": "^2.0.1",
    "lint-staged": "^7.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "preload-webpack-plugin": "^3.0.0-alpha.3",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "serve-static": "^1.13.2",
    "simple-prerender-webpack-plugin": "^3.1.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.0",
    "webpack-manifest-plugin": "^2.0.2",
    "webpack-serve": "^1.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.46",
    "babel-plugin-emotion": "^9.1.0",
    "core-js": "^2.5.3",
    "emotion": "^9.1.3",
    "emotion-server": "^9.1.3",
    "history": "^4.7.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.3",
    "react-hot-loader": "^4.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "regenerator-runtime": "^0.11.1",
    "setprototypeof": "^1.1.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    ".*rc": [
      "prettier --parser json --write",
      "git add"
    ]
  },
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/yarb.git"
  },
  "keywords": [
    "webpack",
    "react",
    "emotion",
    "boilerplate"
  ],
  "author": "dgeibi",
  "bugs": {
    "url": "https://github.com/dgeibi/yarb/issues"
  },
  "homepage": "https://github.com/dgeibi/yarb#readme"
}
